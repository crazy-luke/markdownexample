{
    "name": "MarkdownExample",
    "displayName": "MarkdownExample",
    "description": "full MarkdownExample",
    "publisher": "CrazyLuke",
    "icon": "markdown.png",
    "version": "0.0.2",
    "engines": {
        "vscode": "^1.80.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onStartupFinished"
    ],
    "main": "./extension.js",
    "contributes": {
        "commands": [{
            "command": "markdown.example",
            "title": "MarkdownExample"
        }],
        "menus": {
            "editor/context": [{
                "when": "resourceExtname == .md",
                "command": "markdown.example",
                "group": "navigation"
            }]
        }
    },
    "scripts": {
        "lint": "eslint .",
        "pretest": "npm run lint",
        "test": "node ./test/runTest.js"
    },
    "devDependencies": {
        "@types/vscode": "^1.80.0",
        "@types/glob": "^8.1.0",
        "@types/mocha": "^10.0.1",
        "@types/node": "20.2.5",
        "eslint": "^8.41.0",
        "glob": "^8.1.0",
        "mocha": "^10.2.0",
        "typescript": "^5.1.3",
        "@vscode/test-electron": "^2.3.2"
    }
}